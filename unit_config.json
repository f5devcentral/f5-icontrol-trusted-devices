{
    "routes": [
        {
            "match": {
                "uri": "/TrustedDevices*"
            },
            "action": {
                "pass": "applications/icontrol_trusted_devies"
            }
        },
        {
            "match": {
                "uri": "/TrustedDevicesUI*"
            },
            "action": {
                "pass": "applications/icontrol_trusted_devies"
            }
        },
        {
            "match": {
                "uri": "/TrustedProxy*"
            },
            "action": {
                "pass": "applications/icontrol_trusted_devies"
            }
        }
    ],
    "applications": {
        "icontrol_trusted_devices": {
            "type": "external",
            "working_directory": "/var/lib/f5-icontrol-trusted-devices",
            "executable": "unitapp.js"
        }
    }

}